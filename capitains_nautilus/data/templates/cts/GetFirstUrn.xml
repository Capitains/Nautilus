<GetFirstUrn xmlns="http://chs.harvard.edu/xmlns/cts">
    <request>
        <requestName>GetPrevNext</requestName>
        <requestUrn>{{request_urn}}</requestUrn>
    </request>
    <reply>
        <urn>{{full_urn}}</urn>
        <first>
            <urn>{{full_urn}}:{{firstId}}</urn>
        </first>
    </reply>
</GetFirstUrn>